// Brute force approcach of this Question

 ListNode *getIntersectionNode(ListNode *head1, ListNode *head2) {
        while(head2 != NULL){
            ListNode* temp = head1;
            while(temp != NULL){
                if(temp == head2) return head2;
                temp = temp->next;
            }
            head2 = head2->next;
        }
        
        return NULL;
        
    }
};

